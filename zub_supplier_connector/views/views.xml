<odoo>
  <data>
      <record id="zub_supplier_connector_res_partner_form" model="ir.ui.view">
        <field name="name">zub_supplier_connector_res_partner_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <field name="phone" position="before">
                <field name="is_supplier_with_external_connection"/>
            </field>
        </field>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="ImÃ¡genes">
                    <field name="partner_image_ids" context="{'default_partner_id': id}">
                        <list edit="true" delete="true" create="true" editable="bottom" multi_edit="true" open_form_view="false">
                            <field name="name"/>
                            <field name="image" widget="image"/>
                        </list>
                        <!--<form>
                            <group>
                                <field name="name"/>
                                <field name="image" widget="image"/>
                            </group>
                        </form>-->
                    </field>
                </page>
            </xpath>
        </field>
      </record>
  </data>
</odoo>
